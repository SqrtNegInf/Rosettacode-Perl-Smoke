#!/usr/local/bin/perl
#u# http://rosettacode.org/wiki/Greatest_common_divisor
#c# 2018-08-11 <RC
#p# OK

use feature 'say';
my $result;

# In CORE.  Slowest, takes multiple inputs, result is a Math::BigInt unless converted
use Math::BigInt;
$result .= Math::BigInt::bgcd(49865, 69811)->numify;
$result .= "\n";

# Fastest, takes multiple inputs
use Math::Prime::Util "gcd";
$result .= gcd(49865, 69811);
$result .= "\n";

# Result is a Math::Pari object unless converted
# use Math::Pari "gcd";
# $gcd = gcd(49865, 69811)->pari2iv

say $result;
my $ref = <<'END';
9973
9973
END

use Test::More;
is ($result, $ref);
done_testing();
