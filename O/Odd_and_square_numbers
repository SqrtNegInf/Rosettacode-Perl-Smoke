#!/usr/local/bin/perl
#u# http://rosettacode.org/wiki/Odd_and_square_numbers
#c# 2021-11-26 <RC
#p# OK

my @res;

use strict;
use warnings;
use feature 'say';

use ntheory qw( is_square );

push @res, join( ' ', grep $_ & 1 && is_square($_), 100 .. 999 );

say my $result = join "\n", @res;

use Test::More;
is ($result, '121 169 225 289 361 441 529 625 729 841 961');
done_testing();
