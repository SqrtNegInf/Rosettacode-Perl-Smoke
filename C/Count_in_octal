#!/usr/local/bin/perl
#u# http://rosettacode.org/wiki/Count_in_octal
#t# trivial
#c# 2018-07-02 <RC
#p# OK

my $result;

# Since task says "system register", I take it to mean "no larger than machine native integer limit":

# DH: ha ha ha
#use POSIX;
#printf "%o\n", $_ for (0 .. POSIX::UINT_MAX);

#Otherwise:

use bigint;
my $i = 0;
$result .= sprintf "%o\n", $i++ until $i == 17;

print $result;

my $ref = <<EOD;
0
1
2
3
4
5
6
7
10
11
12
13
14
15
16
17
20
EOD

use Test::More;
is ($result, $ref);
done_testing();
