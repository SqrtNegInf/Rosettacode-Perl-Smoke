#!/usr/local/bin/perl
#u# http://rosettacode.org/wiki/Catalan_numbers
#c# 2018-07-21 <RC
#p# OK

use strict;
use warnings;
use feature 'say';
my $result;
my @c1000;

use ntheory qw/binomial/;

sub catalan {
  my $n = shift;
  binomial(2*$n,$n)/($n+1);
}
push @c1000, catalan($_) for 0 .. 1000; # down from 10000


my $last = '';

use Test::More;
is ($c1000[-1], $last);
done_testing();
