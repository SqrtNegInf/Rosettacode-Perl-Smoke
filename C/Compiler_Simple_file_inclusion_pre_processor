use strict;
use warnings;

our($a,$b,$c);
require './ref/include1.pl';

$a = 'I have a';
print my $result = "$a $b $c\n";

my $ref = "I have a bad feeling about this about this Chewy.\n";

use Test::More;
is($result, $ref);
done_testing();

