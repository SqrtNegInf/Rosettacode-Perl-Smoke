#!/usr/local/bin/perl
#u# http://rosettacode.org/wiki/Create_an_object_at_a_given_address
#t# inprogress
#c# 2019-10-07 >RC
#p# OK

use strict;
use warnings;
use feature 'say';

my @res;

# ~/perl6/Rosetta-Code/X/RC_LOCAL

say my $result = join "\n", @res;
my $ref = <<'END';
END

use Test::More;
chomp $ref;
is($result, $ref);
done_testing();

__END__

